<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterUser</servlet-name>
        <servlet-class>Servlets.RegisterUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValidateUser</servlet-name>
        <servlet-class>Servlets.ValidateUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Prueba</servlet-name>
        <servlet-class>Servlets.Prueba</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Prueba2</servlet-name>
        <servlet-class>Prueba.Prueba2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchUsers</servlet-name>
        <servlet-class>Servlets.SearchUsers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendFriendRequest</servlet-name>
        <servlet-class>Servlets.SendFriendRequest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getFriends</servlet-name>
        <servlet-class>Servlets.getFriends</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Notifications</servlet-name>
        <servlet-class>Servlets.Notifications</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResponseFriendRequest</servlet-name>
        <servlet-class>Servlets.ResponseFriendRequest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getChats</servlet-name>
        <servlet-class>Servlets.getChats</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getDataUser</servlet-name>
        <servlet-class>Servlets.getDataUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterUser</servlet-name>
        <url-pattern>/RegisterUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidateUser</servlet-name>
        <url-pattern>/ValidateUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Prueba</servlet-name>
        <url-pattern>/Prueba</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Prueba2</servlet-name>
        <url-pattern>/Prueba2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchUsers</servlet-name>
        <url-pattern>/SearchUsers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendFriendRequest</servlet-name>
        <url-pattern>/SendFriendRequest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getFriends</servlet-name>
        <url-pattern>/getFriends</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Notifications</servlet-name>
        <url-pattern>/Notifications</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResponseFriendRequest</servlet-name>
        <url-pattern>/ResponseFriendRequest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getChats</servlet-name>
        <url-pattern>/getChats</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getDataUser</servlet-name>
        <url-pattern>/getDataUser</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
